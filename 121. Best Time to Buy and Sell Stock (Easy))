 public int maxProfit(int[] prices) {
        int minP = Integer.MAX_VALUE;
        int maxP = Integer.MIN_VALUE;
        for(int i=0;i<prices.length;i++){
            if(minP >= prices[i]){
                minP = prices[i];
            }
            else{
                maxP = Math.max(maxP,prices[i]-minP);
            }
        }
        return (maxP == Integer.MIN_VALUE ?0:maxP);
}
